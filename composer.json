{
    "name": "hyungjune/reactauth",
    "description": "laravel + react api auth",
    "license": "MIT",
    "authors": [
        {
            "name": "HyungJune Shin",
            "email": "ssa4141@naver.com",
            "homepage": "https://ssa4141.tistory.com"
        }
    ],
    "homepage": "https://github.com/hyungjune/reactauth",
    "keywords": ["Laravel", "ReactAuth"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "laravel/passport": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "hyungjune\\ReactAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hyungjune\\ReactAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "hyungjune\\ReactAuth\\ReactAuthServiceProvider"
            ],
            "aliases": {
                "ReactAuth": "hyungjune\\ReactAuth\\Facades\\ReactAuth"
            }
        }
    }
}
